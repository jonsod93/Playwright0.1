{
  "name": "playwright0.1",
  "version": "1.0.0",
  "main": "hello.js",
  "type": "module",
  "scripts": {
    "pre-run": "node dist/pre-run.cjs",
    "test": "playwright test --reporter=allure-playwright",
    "report": "allure generate --clean allure-results -o allure-report",
    "test:allure": "npm run build && npm run pre-run && (npm run test || set TEST_FAILED=true) && npm run report && npm run serve-report && (if defined TEST_FAILED exit /b 1)",
    "serve-report": "allure open allure-report",
    "lint": "eslint . --ext .ts,.js",
    "format": "prettier --write .",
    "build": "tsc scripts/pre-run.ts --outDir dist && (if exist dist\\pre-run.js (if exist dist\\pre-run.cjs del dist\\pre-run.cjs) & ren dist\\pre-run.js pre-run.cjs)"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/node": "^22.10.7",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "allure-commandline": "^2.33.0",
    "allure-playwright": "^3.2.1",
    "eslint": "^9.25.1",
    "eslint-plugin-playwright": "^2.2.0",
    "playwright": "^1.52.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "allure-js-commons": "^3.2.1",
    "axios": "^1.7.9",
    "dotenv": "^16.5.0",
    "lodash": "^4.17.21",
    "mailisk": "^2.0.1"
  }
}
